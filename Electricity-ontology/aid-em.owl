        <rdfs:subClassOf rdf:resource="http://www.mascem.gecad.isep.ipp.pt/ontologies/aid-em.owl#Tool"/>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="http://www.mascem.gecad.isep.ipp.pt/ontologies/aid-em.owl#hasStrategy"/>
                <someValuesFrom rdf:resource="http://www.mascem.gecad.isep.ipp.pt/ontologies/aid-em.owl#MarketStrategy"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="http://www.mascem.gecad.isep.ipp.pt/ontologies/aid-em.owl#hasStrategy"/>
                <someValuesFrom rdf:resource="http://www.mascem.gecad.isep.ipp.pt/ontologies/aid-em.owl#RLAStrategy"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">This class represents an abstract ALBidS. The ALBidS tool contains Market Strategies and RLA Strategies that are designed to help players that participate in day-ahead market type.</rdfs:comment>
    </Class>
    


